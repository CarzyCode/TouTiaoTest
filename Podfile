# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'TouTiao' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TouTiao
  pod 'Alamofire'   	# 数据请求 https://github.com/Alamofire/Alamofire，同 AFNetworking
  pod 'HandyJSON', '~> 5.0.1'     # JSON序列化/反序列化库 https://github.com/alibaba/HandyJSON/
  pod 'SwiftyJSON', '~> 4.2.0'    # json 解析 https://github.com/SwiftyJSON/
   pod 'SGPagingView', '~> 1.6.0'
  pod 'SnapKit', '~> 4.2.0'  # 自动布局框架，类似于Masonry。（推荐）
  pod 'Kingfisher', '~> 5.6.0'

  target 'TouTiaoTests' do
    inherit! :search_paths
    # Pods for testing
    
  end

  target 'TouTiaoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
